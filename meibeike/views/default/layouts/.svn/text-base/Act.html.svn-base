		// 验证码长度
		var input_cap_just_change = 0;
		function validate_cap_length(obj) {
			if ($(obj).attr("maxlength") == "") {
				return;
			}
			
			if (input_cap_just_change == 1) {
				return true;
			}
			input_cap_just_change = 1;
		
			var len = $(obj).val().length;
			if (len > parseInt($(obj).attr("maxlength"))) {
				var value_new = $(obj).val().substring(0, $(obj).attr("maxlength"));
				$(this).val(value_new);
			}
		
			input_cap_just_change = 0;
		}
		$("input[name=captcha]").bind('input propertychange', function(event) {
			validate_cap_length($(this));
		});  
		$("input[name=vcode]").bind('input propertychange', function(event) {
			//alert("dd")
			validate_cap_length($(this));
		});  
		
